<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-3.0.xsd"
	default-autowire="byName">

	<bean id="employeeDAO" class="com.jboa.dao.impl.EmployeeDAOImpl">
		<property name="sqlSessionTemplate" ref="sqlSessionTemplate"></property>
	</bean>
	<bean id="claimVoucherStaticsDAO" class="com.jboa.dao.impl.ClaimVoucherStaticsDAOImpl">
		<property name="sqlSessionTemplate" ref="sqlSessionTemplate"></property>
	</bean>
	<bean id="checkResultDAO" class="com.jboa.dao.impl.CheckResultDAOImpl">
		<property name="sqlSessionTemplate" ref="sqlSessionTemplate"></property>
	</bean>
	<bean id="positionDAO" class="com.jboa.dao.impl.PositionDAOImpl">
		<property name="sqlSessionTemplate" ref="sqlSessionTemplate"></property>
	</bean>
	<bean id="claimVoucherDAO" class="com.jboa.dao.impl.ClaimVoucherDAOImpl">
		<property name="sqlSessionTemplate" ref="sqlSessionTemplate"></property>
	</bean>
	<bean id="leaveDAO" class="com.jboa.dao.impl.LeaveDAOImpl">
		<property name="sqlSessionTemplate" ref="sqlSessionTemplate"></property>
	</bean>
	<bean id="claimVoucherDetailDAO" class="com.jboa.dao.impl.ClaimVoucherDetailDAOImpl">
		<property name="sqlSessionTemplate" ref="sqlSessionTemplate"></property>
	</bean>
	<bean id="departmentDAO" class="com.jboa.dao.impl.DepartmentDAOImpl">
		<property name="sqlSessionTemplate" ref="sqlSessionTemplate"></property>
	</bean>
</beans>